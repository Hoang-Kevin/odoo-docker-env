<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1"> 

        <record id="the_easy_delivery" model="product.product">
            <field name="name">The Easy Delivery</field>
            <field name="default_code">Easy_Delivery</field>
            <field name="type">service</field>
            <field name="categ_id" ref="delivery.product_category_deliveries"/>
            <field name="sale_ok" eval="False"/>
            <field name="purchase_ok" eval="False"/>
            <field name="list_price">0.0</field>
            <field name="invoice_policy">order</field>
        </record>

        <record id="delivery_carrier" model="delivery.carrier">
            <field name="name">The Easy Delivery</field>
            <field name="fixed_price">0.0</field>
            <field name="sequence">2</field>
            <field name="delivery_type">easy_delivery</field>
            <field name="product_id" ref="easy_delivery_connector.the_easy_delivery"/>
        </record>
    </data>
</odoo>